#include <iostream>
#include <fstream>
#include <string>
#include <stdlib.h> 
using namespace std;

int main(){
	int x = 10;
	int y = 10;
	for(int i = 0; i<y; i++){
		endl;
	}
	for(int i = 0; i<x; i++){
		cout << " ";
	}






	string input;
	while(true){

	for(int i = 0; i<y; i++){
		endl;
	}
	for(int i = 0; i<x; i++){
		cout << " ";
	}

	cout << "Q";
		cin >> input;

	if(!input.compare("w") ){
		y++;
	}
	if(!input.compare("a") ){
		x--;
	}
	if(!input.compare("s") ){
		y--;
	}
	if(!input.compare("d") ){
		x++;
	}

	}
}